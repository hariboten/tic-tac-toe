{
  "name": "tic-tac-toe",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "ng": "ng",
    "prestart": "node scripts/sync-version.mjs",
    "start": "ng serve",
    "prebuild": "node scripts/sync-version.mjs",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --runInBand",
    "build:gh-pages": "ng build --configuration production --base-href /tic-tac-toe/",
    "version:sync": "node scripts/sync-version.mjs",
    "version:bump": "node scripts/bump-version.mjs patch",
    "version:bump:minor": "node scripts/bump-version.mjs minor",
    "version:bump:major": "node scripts/bump-version.mjs major"
  },
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.14",
    "jasmine-core": "~5.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.5.3",
    "jsdom": "^28.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-jsdom-launcher": "^20.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "~5.6.2"
  }
}
